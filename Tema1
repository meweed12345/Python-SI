my_list = [7, 8, 9, 2, 3, 1, 4, 10, 5, 6]
print("My list")
print(my_list)

my_list1 = sorted(my_list)
print("Ascendent order")
print(my_list1)

my_list2 = my_list1[:: -1]
print("Descendent order")
print(my_list2)

print("Even list")
print(my_list1[1:len(my_list1):2])

print("Odd list")
print(my_list1[0:len(my_list)-1:2])


print("Multiple of 3")
print(my_list1[2:len(my_list1)-1:3])
